/**
*  请定义在上面的模版里 按照 ·$u-属性-值· 命名如 $u-font-sm
*/
/** 健康块 */
.app-section {
  border-radius: 10px;
  background: #fff;
  padding: 14px;
  margin-top: 14px;
  padding-top: 0px;
}
.app-section .app-section-header {
  width: 100%;
  height: 52px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  border-bottom: 1px #eeeeee solid;
}
.app-section .app-section-header .app-section-header-title {
  font-size: 16px;
  font-weight: 600;
}
.app-section .app-section-header .app-section-header-btn {
  border: 1px #e8e8e8 solid;
  border-radius: 14px;
  height: 28px;
  font-size: 14px;
  padding: 0 10px;
  color: #3873fe;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.app-section .app-section-bodyer {
  width: 100%;
}
/** 健康颜色定义 */
.color-warning {
  color: #EE9E5E;
}
.color-health {
  color: #4DAD9B;
}
.color-risk {
  color: #F4C264;
}
.color-danger {
  color: #EA6D2C;
}
/** 健康颜色背景 **/
.bg-warning {
  background-color: #fcc14f;
}
.bg-health {
  background-color: #01b09a;
}
.bg-risk {
  background-color: #F4C264;
}
.bg-danger {
  background-color: #fa6400;
}
page {
  background: #fafafa;
}
.app-check-items-detail .app-check-items-detail-section {
  margin-bottom: 14px;
}
.app-check-items-detail .app-check-items-detail-section .app-check-items-detail-header {
  padding: 14px 14px;
}
.app-check-items-detail .app-check-items-detail-section .app-check-items-detail-header .title {
  font-size: 18px;
  font-weight: 600;
}
.app-check-items-detail .app-check-items-detail-section .app-check-items-detail-header .des {
  font-size: 14px;
  color: #999;
  margin-top: 8px;
}
.app-check-items-detail .app-check-items-detail-section .app-check-items-detail-section-group {
  overflow: hidden;
}
.sub {
  font-size: 10rpx;
  position: relative;
  color: rgb(194, 193, 193);
}
