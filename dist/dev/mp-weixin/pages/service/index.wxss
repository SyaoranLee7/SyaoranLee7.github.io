.app-home {
  min-height: 60vh;
  background-position: top right;
  padding: 20px 14px;
  height: 200px;
}
.app-home-plan-items {
  margin-top: 10px;
}
.app-home-plan-items image {
  width: 100%;
}
.app-home-title {
  height: 90px;
  color: #fff;
}
.app-home-title ._h1 {
  font-size: 28px;
  font-weight: bold;
  margin-top: 20px;
}
.app-home-title ._p {
  font-size: 14px;
}
/** 健康目标 */
